## S100_PR_Alert
{
  _id: ObjId,
  kind: "S100_PR_Alert",

  S100_PR_RegisterItem: { xmlID: string, description: S100_PR_NationalLanguageString[] },

  S100_PR_Alert: {
    routeMonitor: S100_PR_AlertInfo[], // 0..*
    routePlan: S100_PR_AlertInfo[]     // 0..*
  }
}

## S100_PR_AlertInfo
{
  priority: S100_PR_AlertPriority[] // 1..*  (원문 그대로 반영)
}

## S100_PR_AlertPriority
{
  priority: "alarm"|"warning"|"caution"|"indication", // S100_PR_AlertPriorityType
  default: boolean,  // 0..1
  optional: boolean  // 0..1
}

## S100_PR_AlertMessage
{
  text: S100_PR_NationalLanguageString[], // 1..*
  icon: string // 0..1  (연관관계 없으니 symbol의 xmlID 같은 식별자 문자열로 저장 추천)
}

## S100_PR_AlertHighlight
{
  optional: boolean, // 0..1
  style: "AlarmHighlight"|"CautionHighlight" // 0..1 (S100_PR_HighlightStyle)
}

