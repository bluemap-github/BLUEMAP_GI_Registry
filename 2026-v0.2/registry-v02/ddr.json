## S100_CD_ComplexAttribute

{
  _id : ObjId, 
  kind : "S100_CD_ComplexAttribute"
  concept : {
    itemIdentifier: Integer (하지만 아래 3번 규칙에 따라 “string으로 저장”이 원칙이면 string 처리)
    name: string
    definition: string
    remarks: string
    itemStatus: (enumeration)
    alias: string[]
    camelCase: string
    definitionSource: string
    reference: string
    similarityToSource: string
    justification: string
    proposedChange: string
  }, 
  S100_CD_ComplexAttribute : {
    "subAttributes": [
      {
        "attributeIdentifier": "string", // 하위 속성의 camelCase 혹은 ID 참조
        "multiplicity": {
          "lower": 1,                // 최소 발생 횟수 (S100_Multiplicity)
          "upper": 1,                // 최대 발생 횟수 (null일 경우 무한대)
          "infinite": false          // 무한대 여부 [2, 3]
        },
        "sequential": false          // 하위 속성 간 순서의 중요성 여부 [4]
      },
      {
        "attributeIdentifier": "otherAttributeCode",
        "multiplicity": {
          "lower": 0,
          "upper": null,
          "infinite": true
        },
        "sequential": true           // 동일 속성이 여러 개일 때 순서가 의미 있음 [4]
      }
    ]
  }
}

